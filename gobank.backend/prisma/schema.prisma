// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Users {
  user_id Int @id @default(autoincrement())
  user_name String
  user_password String
  user_email String
  user_recovery String
  user_phone String
  
}

model Personal {
  personal_account Int @id @unique
  personal_fname String
  personal_lname String
  personal_age  String
  personal_profilePic String
}

model Bank {
  bank_id Int @id @unique
  bank_agency String
  bank_name String
  bank_account String
  bank_number String
}